echo
read -sn 1 -p "Press any key to continue..."

for i in `seq 1 $2`;
do
clear
echo "*********************************** REQUEST # $i"
echo
echo R | curl https://cnsabadell.deporsite.net 2>&1;
COUNT=`expr $COUNT + 1`
done
